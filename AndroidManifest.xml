<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodreference2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.foodreference2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.foodreference2.Reference"
            android:label="@string/title_activity_reference"
            android:parentActivityName="com.example.foodreference2.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.foodreference2.MainActivity" />
        </activity>
        <activity
            android:name="com.example.foodreference2.Track"
            android:label="@string/title_activity_track" 
             android:parentActivityName="com.example.myfirstapp.MainActivity" >        
                <meta-data            android:name="android.support.PARENT_ACTIVITY"            
                android:value="com.example.myfirstapp.MainActivity" />  
        </activity>
        <activity
            android:name="com.example.foodreference2.Result"
            android:label="@string/title_activity_result" 
            android:parentActivityName="com.example.foodreference2.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.foodreference2.Reference" />
        </activity>
    </application>

</manifest>